<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.edu.ncist.kb.dao.ScheduleDao">
    <select id="queryListByXHs" resultType="cn.edu.ncist.kb.po.SchedulePO">
        SELECT * FROM schedule WHERE USER_XH IN
        <foreach collection="xhs" item="item" open="(" close=")" separator=",">#{item}</foreach>
    </select>
</mapper>
